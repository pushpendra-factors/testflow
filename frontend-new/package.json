  "scripts": {
    "dev": "export NODE_ENV=development && ./node_modules/.bin/webpack-dev-server --host factors-dev.com --port 3000 --content-base src --inline --hot",
    "dev-https": "HTTPS=true SSL_CRT_FILE=localhost.pem SSL_KEY_FILE=localhost-key.pem npm run dev",
    "copy-sdk-to-staging": "cd $FACTORS_REPO/sdk/javascript; npm run build-prod; make deploy-staging; cp $FACTORS_REPO/sdk/javascript/dist/factors.v1.js $FACTORS_REPO/frontend-new/dist/staging/assets/v1/factors.js",
    "copy-old-sdk-to-staging": "cd $FACTORS_REPO/sdk/javascript; npm run build-prod-v0; cp $FACTORS_REPO/sdk/javascript/dist/factors.v0.js $FACTORS_REPO/frontend-new/dist/staging/assets/factors.js",
    "copy-sdk-to-prod": "cd $FACTORS_REPO/sdk/javascript; npm run build-prod; make deploy-prod; cp $FACTORS_REPO/sdk/javascript/dist/factors.v1.js $FACTORS_REPO/frontend-new/dist/production/assets/v1/factors.js",
    "copy-old-sdk-to-prod": "cd $FACTORS_REPO/sdk/javascript; npm run build-prod-v0; cp $FACTORS_REPO/sdk/javascript/dist/factors.v0.js $FACTORS_REPO/frontend-new/dist/production/assets/factors.js",
    "build-staging": "export NODE_ENV=staging; rm -rf ./dist/staging; mkdir -p ./dist/staging; mkdir -p ./dist/staging/assets/v1; ./node_modules/.bin/webpack -p --config webpack.config.js; npm run copy-old-sdk-to-staging; npm run copy-sdk-to-staging",
    "build-staging-no-deployment": "export NODE_ENV=staging; rm -rf ./dist/staging; mkdir -p ./dist/staging; mkdir -p ./dist/staging/assets/v1; ./node_modules/.bin/webpack -p --config webpack.config.js;",
    "build-prod": "export NODE_ENV=production; rm -rf ./dist/production; mkdir -p ./dist/production; mkdir -p ./dist/production/assets/v1; ./node_modules/.bin/webpack -p --config webpack.config.js; npm run copy-sdk-to-prod; npm run copy-old-sdk-to-prod",
    "build-prod-no-deployment": "export NODE_ENV=production; rm -rf ./dist/production; mkdir -p ./dist/production; mkdir -p ./dist/production/assets/v1; ./node_modules/.bin/webpack -p --config webpack.config.js;",
    "build-docker": "export NODE_ENV=development; rm -rf ./dist/development; mkdir -p ./dist/development; ./node_modules/.bin/webpack -p --config webpack.config.js",
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "jest",
    "coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/tailwind.output.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/tailwind.output.css",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "analyze": "npm run dev --analyze"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
