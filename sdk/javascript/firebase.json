{
    "hosting": [
      {
        "target": "staging",
        "public": "dist",
        "ignore": [
          "firebase.json",
          "**/.*",
          "**/node_modules/**"
        ],
        "rewrites": [
          {
            "source": "**",
            "destination": "/index.html"
          }
        ],
        "headers" : [
          {
            "source" : "factors.v1.js",
            "headers" : [ 
              {
                "key" : "Cache-Control",
                "value" : "public, max-age=3600, s-maxage=3600"
              }
            ]
          }
        ]
      },
      {
        "target": "production",
        "public": "dist",
        "ignore": [
          "firebase.json",
          "**/.*",
          "**/node_modules/**"
        ],
        "rewrites": [
          {
            "source": "**",
            "destination": "/index.html"
          }
        ],
        "headers" : [
          {
            "source" : "factors.v1.js",
            "headers" : [ 
              {
                "key" : "Cache-Control",
                "value" : "public, max-age=3600, s-maxage=3600"
              }
            ]
          }
        ]
      }
    ]
  }
  